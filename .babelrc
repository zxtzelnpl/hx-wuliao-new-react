{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2",
    "react"
  ],
  "env": {
    "dev": {
      "plugins": [
        ["react-transform",{"transform": [{"transform": "react-transform-hmr","import": ["react"],"local": ["module"]}]}]
      ]
    }
  },
  "plugins": [
    "transform-object-rest-spread"
  ]
}
